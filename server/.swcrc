{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": null,
    "target": "es2020",
    "loose": false,

    "externalHelpers": false,

    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
  },
  "module": {
    "type": "commonjs",
    "ignoreDynamic": true
  },
  "minify": false
}
